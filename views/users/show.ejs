<h2 style="margin-top: 60px">Hello, <%= loggedIn.username %> (<%= loggedIn.email %>)!</h2>

<h3>The user you have clicked on has added the following articles:</h3>

<ul>
  <% articles.forEach(article => { %>
    <li><a href="/articles/<%= article._id %>"><%= article.name %></a></li>
  <% }); %>
</ul>
